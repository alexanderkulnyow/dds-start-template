{
  "name": "alexanderkulnyow/dds-start-template",
  "type": "wordpress-theme",
  "description": "Hi. I'm a starter theme called Dds_Start_Template, based on underscores",
  "keywords": [
    "WordPress",
    "Themes"
  ],
  "homepage": "https://github.com/alexanderkulnyow/dds-start-template",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Contributors",
      "homepage": "https://github.com/alexanderkulnyow/dds-start-template/graphs/contributors"
    }
  ],
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^v0.7.1",
    "wptrt/wpthemereview": "^0.2.1",
    "php-parallel-lint/php-parallel-lint": "^1.2.0",
    "wp-cli/i18n-command": "^2.2",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "scripts": {
    "lint:wpcs": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcs",
    "lint:phpcbf": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcbf",
    "lint:php": "@php ./vendor/bin/parallel-lint --exclude .git --exclude vendor .",
    "make-pot": "wp i18n make-pot . languages/dds-start-template.pot"
  },
  "support": {
    "issues": "https://github.com/alexanderkulnyow/dds-start-template/issues",
    "source": "https://github.com/alexanderkulnyow/dds-start-template"
  }
}
